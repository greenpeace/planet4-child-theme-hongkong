{
	"name": "greenpeace/gpea-planet8-child-theme",
	"description": "Child theme for the GPEA P8 website of the Planet 4 Wordpress project",
	"type":"wordpress-theme",
	"minimum-stability" : "stable",
	"authors": [
		{
			"name": "Konstantinos Kokkorogiannis",
			"email": "kkokkoro@greenpeace.org"
		}
	],
	"license": "GPL-3.0+",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"support": {
		"issues": "https://github.com/greenpeace/gpea-planet8-child-theme/issues",
		"wiki": "https://github.com/greenpeace/gpea-planet8-child-theme/wiki",
		"source": "https://github.com/greenpeace/gpea-planet8-child-theme"
