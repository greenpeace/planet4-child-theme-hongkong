{% block html_head_container %}

{% include 'html-header.twig' %}

{% endblock %}

<body class="{{ body_class }} {{ custom_body_classes|default("") }}" {{ body_extra_info|raw }}>

	{% if google_tag_value %}
		<!-- Google Tag Manager (noscript) -->
	    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id={{ google_tag_value }}"
			      height="0" width="0" style="display:none;visibility:hidden"></iframe>
	    </noscript>
	    <!-- End Google Tag Manager (noscript) -->
	{% endif %}

	{% include 'navigation-bar.twig' with {
		data_nav_bar: data_nav_bar,
		strings: strings_navbar,
	} %}
	{% include 'sidebar.twig' with data_nav_bar %}
	{#% include 'blocks/header.twig' %#}

	<div class="outer_block_container">
	    {% block content %}
		Sorry, no content
	    {% endblock %}
	</div>

	{% if sidebar %}
		<aside class="layout-sidebar">
			{{sidebar}}
		</aside>
	{% endif %}

	{% block footer %}
		{% include 'footer.twig' with {
			strings: strings_footer,
		} %}
		{#% block cookies %#}
			{#% include 'cookies.twig' %#}
		{#% endblock %#}
		{{ function('wp_footer') }}
	{% endblock %}
</body>
</html>
